{
	"name": "filesac",
	"version": "14.2.0",
	"description": "File utility functions",
	"license": "CC0-1.0",
	"type": "module",
	"main": "files.js",
	"scripts": {
		"test": "node tests/files-test.js",
		"lint-fix": "eslint --ignore-path .gitignore --fix files.js",
		"lint": "eslint --ignore-path .gitignore files.js"
	},
	"devDependencies": {
		"eslint": "^8.2.0",
		"eslint-config-red": "^1.8.2"
	},
	"eslintConfig": {
		"extends": [
			"red"
		],
		"parserOptions": {
			"ecmaVersion": 2021,
			"sourceType": "module"
		},
		"env": {
			"node": true,
			"es2021": true
		}
	},
	"files": [
		"files.js",
		"readme.me",
		"changelog.md",
		"license.txt"
	],
	"repository": {
		"type": "git",
		"url": "git://github.com/GrosSacASac/filesac.git"
	},
	"keywords": [
		"utility",
		"file",
		"promise",
		"copy",
		"directory",
		"file",
		"write",
		"read",
		"concatenate"
	]
}
